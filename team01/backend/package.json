    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.15.1",
    "multer": "^2.0.1",
    "nodemailer": "^6.9.1",
    "socket.io": "^4.8.1",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "chai": "^4.4.1",
    "chai-http": "^4.4.0",
    "cross-env": "^7.0.3",
    "jsdom": "^26.1.0",
    "mocha": "^11.7.1",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.10",
    "nyc": "^17.1.0",
    "sinon": "^21.0.0",
    "supertest": "^7.1.1",
    "vitest": "^3.2.4"
  }
